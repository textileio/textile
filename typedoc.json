{
  "readme": "./README.md",
  "name": "Threads",
  "mode": "modules",
  "out": "docs",
  "include": "src/",
  "exclude": ["**/*.spec.ts", "examples/**", "**/node_modules/**"],
  "gitRevision": "master",
  "excludePrivate": true,
  "excludeNotExported": true,
  "excludeProtected": true,
  "hideGenerator": true,
  "target": "ES6",
  "moduleResolution": "node",
  "preserveConstEnums": true,
  "stripInternal": true,
  "module": "commonjs",
  "mdHideSources": true,
  "pathExclude": ["examples/**"]
}
