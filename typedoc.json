{
  "readme": "./README.md",
  "name": "Textile",
  "mode": "modules",
  "out": "docs",
  "lib": [
    "es6",
    "dom",
    "esnext"
  ],
  "exclude": ["**/*.spec.ts", "**/spec.util.ts", "examples/**", "**/node_modules/**"],
  "gitRevision": "master",
  "excludePrivate": true,
  "excludeNotExported": true,
  "excludeProtected": true,
  "hideGenerator": true,
  "target": "es6",
  "moduleResolution": "node",
  "preserveConstEnums": true,
  "stripInternal": true,
  "module": "commonjs",
  "mdHideSources": true
}
